//团队格式化规范 airbnb-base + eslint + prettier + volar
{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false, //true：普通函数名后面加空格
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false //true：匿名函数后面加空格
}
